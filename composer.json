{
    "name": "app/foundation",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
